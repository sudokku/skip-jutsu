{
    "manifest_version": 3,
    "name": "Skip Jutsu",
    "version": "1.1",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://jut.su/*",
        "ws://localhost:8765/*",
        "http://localhost:8765/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://jut.su/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}